<!DOCTYPE html>

@{
    ViewBag.Title = "About";
    Layout = null;
}
@Html.Raw(TempData["msg"])

<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
