    <style>
        .serv1 {
            width: 200px;
            height: 200px;
        }

            /* resize images */
            .serv1 img {
                width: 200%;
                height: 200%;
            }
    </style>

    <style>
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
            overflow: hidden;
            /*border: 1px solid #e7e7e7;*/
            /*background-color: #f3f3f3;*/
        }

        li {
            float: left;
        }

            li a {
                display: block;
                color: #b6ff00;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
            }

                li a:hover:not(.active) {
                    background-color: #ddd;
                }

                li a.active {
                    color: white;
                    background-color: #4CAF50;
                }
