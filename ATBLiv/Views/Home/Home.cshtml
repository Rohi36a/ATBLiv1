            </button>
            <div id="Arise Tech Solutions-navbar" class="collapse navbar-collapse">
                <ul class="navbar-nav ml-auto">
                    <li class="active">
                        @Html.ActionLink("Home", "Home", "Home")
                    </li>
                    <li>
                        @Html.ActionLink("About", "About", "Home")
                    </li>
                    <li>
                        @Html.ActionLink("Services", "Services", "Home")
                    </li>
                    <li>
                        @Html.ActionLink("Products", "Products", "Home")
                    </li>
                    <li>
                        @Html.ActionLink("Partners", "Partners", "Home")
                    </li>
                    <li>
                        @Html.ActionLink("Contact", "Contact", "Home")
                    </li>
