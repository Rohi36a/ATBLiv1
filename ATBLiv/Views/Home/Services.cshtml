@using (Html.BeginForm("email", "Home", FormMethod.Post))
    {

    <section>
        <div class="container">
            <div class="row">
                <div class="col-md-5 mt-3">
                    <h3>
                        <i class="fa fa-pencil"></i> Quick Quote
                    </h3>
                    <div class="block-content">
                        <form role="form">
                            <div class="form-group">
                                <label>Name</label>
                                <input type="text" class="form-control" name="sname">
                            </div>
                            <div class="form-group">
                                <label>Phone Number</label>
                                <input type="text" class="form-control" name="sphone">
                            </div>
                            <div class="form-group">
                                <label>Email address</label>
                                <input type="email" class="form-control" name="semail">
                            </div>
                            <div class="form-group">
                                <label>Message</label>
                                <textarea class="form-control" name="smessage"></textarea>
                            </div>
                           @*<button type="submit" class="btn btn-primary">Submit</button>*@
                            <button type="submit" class="btn btn-xl" id="submit" onclick="this.form.submit();">Send Message</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    
}
